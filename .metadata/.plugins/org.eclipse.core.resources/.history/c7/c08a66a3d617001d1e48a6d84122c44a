package pacote;

import java.util.Arrays;

public class PrimeiraAtiv {
	static int lisT[] = new int[6]; //Cria lista de valores 
	static int matrz[][] = new int[6][6]; //Cria matriz de valores calculados
	
	public static void main(String[] args) {
		// Inicio
		PrimeiraAtiv(5,6,7,8,9,10);
		System.out.println("Esta Ã© a lista de valores: ");
		System.out.println(Arrays.toString(lisT));
		ConstroeMtz(lisT);
		
		
	}
	private static void PrimeiraAtiv(int i, int j, int k, int l, int m, int n) {
		lisT[0] = i;
		lisT[1] = j;
		lisT[2] = k;
		lisT[3] = l;
		lisT[4] = m;
		lisT[5] = n;
		
		
	}
	
	

	private static void ConstroeMtz(int lista[]) {
		for(int i=0; i < 6;i++) {
			for(int j=0; j < 6;j++) {
				matrz[i][j]= lisT[i] * lisT[j];
		System.out.println("Matriz composta pela lista dada:");
		System.out.println(Arrays.toString(matrz));
				
			}
		}
		
	}
/*
	private static void mediaMtz(float med) {
		med = 0;
		for(int i=0; i < 6; i++) {
			for(int j=0; j < 6; j++) {
				med = med + mtz
			}
		}
	}*/

}

