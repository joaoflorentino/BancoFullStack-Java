package pacote;

import java.util.Arrays;

public class PrimeiraAtiv {
	static int lisT[] = new int[6]; //Cria lista de valores 
	int matrz[][] = new int[6][6]; //Cria matriz de valores calculados
	
	public static void main(String[] args) {
		// Inicio
		System.out.println("Esta Ã© a lista de valores: ");
		System.out.println(Arrays.toString(lisT));
		
	}
	public PrimeiraAtiv(int a, int b, int c, int d, int e, int f) {
		this.lisT[0] = a;
		this.lisT[1] = b;
		this.lisT[2] = c;
		this.lisT[3] = d;
		this.lisT[4] = e;
		this.lisT[5] = f;
	
	}
	

	private static int[][] constroeMtz(int mtz[][]) {
		for(int i=0; i < 6;i++) {
			for(int j=0; j < 6;j++) {
				mtz[i][j]= lisT[i] * lisT[j];
		System.out.println("Matriz composta pela lista dada:");
		System.out.println(Arrays.toString(mtz));
		return mtz;
				
			}
		}
		return mtz;
	}
/*
